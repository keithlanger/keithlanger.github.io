{
    "routes": [
        {
            "route": "/bsdemo1/assets/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/flightdemo1/assets/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/flightclient/assets/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/flightdemo2/assets/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/bsdemo1/*",
            "serve": "/bsdemo1/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightdemo1/*",
            "serve": "/flightdemo1/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightclient/*",
            "serve": "/flightclient/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightdemo2/*",
            "serve": "/flightdemo2/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,json}",
            "/api/*",
            "/bsdemo1/assets/*",
            "/flightdemo1/assets/*",
            "/flightclient/assets/*",
            "/flightdemo2/assets/*",
            "/bsdemo1/*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,json}",
            "/flightdemo1/*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,json}",
            "/flightclient/*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,json}",
            "/flightdemo2/*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot,json}"
        ]
    },
    "mimeTypes": {
        ".css": "text/css",
        ".js": "application/javascript",
        ".json": "application/json",
        ".woff": "font/woff",
        ".woff2": "font/woff2",
        ".ttf": "font/ttf",
        ".eot": "application/vnd.ms-fontobject",
        ".svg": "image/svg+xml"
    }
}