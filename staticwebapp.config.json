{
    "routes": [
        {
            "route": "/bsdemo1/*",
            "serve": "/bsdemo1/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightdemo1/*",
            "serve": "/flightdemo1/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightclient/*",
            "serve": "/flightclient/index.html",
            "statusCode": 200
        },
        {
            "route": "/flightdemo2/*",
            "serve": "/flightdemo2/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["*.{css,js,map,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}", "/api/*"]
    }
}